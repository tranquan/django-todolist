{
  // "python.pythonPath": "~/.virtualenvs/todolist/bin/python",
  // "python.linting.pylintPath": "~/.virtualenvs/todolist/bin/pylint",
  "python.linting.pylintEnabled": true,
  "python.linting.pylintArgs": [
    "--disable=django-not-configured",
    "--django-settings-module=<mainapp>.settings",
  ],
  "python.analysis.extraPaths": [
    "todolist"
  ],
  "typescriptHero.imports.disableImportRemovalOnOrganize": false,
  "typescriptHero.imports.multiLineWrapThreshold": 1000,
  "typescriptHero.imports.organizeOnSave": true,
  "typescriptHero.imports.insertSemicolons": true,
  "typescriptHero.imports.insertSpaceBeforeAndAfterImportBraces": true,
  "typescriptHero.imports.stringQuoteStyle": "\"",
  "typescriptHero.imports.grouping": [
    {
      "identifier": "/^(react-native-gesture-handler|react|react-native)$/",
      "order": "asc"
    },
    "Modules",
    {
      "identifier": "/^(src|app|constants|shared)/",
      "order": "asc"
    },
    "Workspace",
    "Plains",
    "Remaining"
  ],
}